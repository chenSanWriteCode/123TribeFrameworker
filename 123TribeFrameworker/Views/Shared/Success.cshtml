

<div class="alert alert-success with-icon">
    <i class="icon-ok-sign"></i>
    @if (ViewBag.Msg==null)
    {
        <div class="content">操作成功！</div>
    }
    else
    {
        <div class="content">@ViewBag.Msg</div>
    }
    
</div>
